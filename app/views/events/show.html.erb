<% content_for :meta_title, "#{@event.trip.name} || #{DEFAULT_META["meta_product_name"]}" %>
<div class="container">
  <div class="left-align" style="margin-top: 50px">
    <div class="row header mt-5">
      <div class="col-8">
        <h2>
          <% if ["journey", "flight", "train" , "bus" , "boat"].include? @event.event_type %>
            <%= @event.event_type.capitalize %> Details
          <% elsif @event.event_type == "rental" %>
            Rental <%= "with #{@event.provider}" if @event.provider %>
          <% elsif @event.event_type == "stay" %>
            Stay <%= "at #{@event.provider}" if @event.provider %>
          <% elsif @event.event_type == "restaurant" %>
            Reservation <%= "at #{@event.event_provider}" if @event.provider %>
          <% elsif @event.event_type == "visit" %>
            Visit to <%= @event.event_name %>
          <% elsif @event.event_type == "show" %>
            Show: <%= @event.event_name %> <%= "at #{@event.provider}" if @event.provider %>
          <% elsif @event.event_type == "other" %>
            <%= @event.name %>
          <% end %>
        </h2>
      </div>
      <div class="edit-button col-2">
        <%= link_to edit_trip_event_path(@trip, @event) do %>
          <i class="fa-regular fa-pen-to-square fa-2xl" style="color: #7f7eff;"></i>
        <% end %>
      </div>
      <div class="delete-button col-2">
        <%= link_to trip_event_path(@trip, @event), data: {turbo_method: :delete, turbo_confirm: "All event data, and all event tasks and files will be deleted too."} do %>
          <i class="fa-regular fa-trash-can fa-2xl" style="color: #7f7eff;"></i>
        <% end %>
      </div>
    </div>
  </div>
