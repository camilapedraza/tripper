    <div class="documents">
      <% @event.files.each do |file| %>
        <a href="" class="secondary-pill-button document"><%= file.filename.base.upcase %></a>
      <% end %>
      <%= link_to "ADD FILE", add_file_path(@event.trip, @event), class: "primary-pill-button document" %>
    </div>
  </div>

  <div class="left-align">
    <div class="checklist-container">
      <h2>Checklist</h2>
        <div class="checklist-items">
          <ul>
            <% @event.tasks.each do |task| %>
              <li style="list-style-type:none" class="checklist-item">
                <% if task.is_complete %>
                  <%= link_to task_path(task, params: {task: {is_complete: false}}), data: {turbo_method: :patch} do %>
                    <svg style="margin-right: 12px" width="22" height="22" viewBox="0 0 22 22" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <circle cx="11" cy="11" r="10.5" fill="#7F7EFF" stroke="#7F7EFF"/>
                      <path d="M7 12L10 14.5L15 8" stroke="white" stroke-linecap="round"/>
                    </svg>
                  <% end %>
                    <span><%= task.content %></span>
                <% else %>
                  <%= link_to task_path(task, params: {task: {is_complete: true}}), data: {turbo_method: :patch} do %>
                  <svg style="margin-right: 12px" width="22" height="22" viewBox="0 0 22 22" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <circle id="Ellipse 10" cx="11" cy="11" r="10.5" stroke="#7F7EFF"/>
                  </svg>
                  <% end %>
                  <%= task.content %>
                <% end %>
                &nbsp;
                <%= link_to task_path(task), data: {turbo_method: :delete} do %>
                  <i class="fa-solid fa-xmark"></i>
                <% end %>
              </li>
            <% end %>
          </ul>
          <div class="checklist">
            <%= simple_form_for [@event, @task], html: { id: 'add-task-form' } do |f| %>
              <div class="add-task-button m-0">
                <%= f.button :button, class: "add-task-button-down" do %>
                  <svg width="15" height="16" viewBox="0 0 15 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path id="Vector 6" d="M7.5 1V7.75M7.5 14.5V7.75M7.5 7.75H14H1" stroke="#7F7EFF" stroke-width="2" stroke-linecap="round"/>
                  </svg>
                <% end %>
                <%= f.input :content, label: false, placeholder: 'add something...', input_html: { class: 'add-task-input' } %>
              </div>
            <% end %>
          </div>
        </div>
        <div>
        </div>
    </div>
  </div>
</div>
